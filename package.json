    ],
    "scripts": {
        "client": "webpack-dev-server --progress --colors --inline --hot",
        "server": "nodemon server/app.js --exec babel-node",
        "build-client": "webpack --display-error-details",
        "build-server": "babel server --out-dir dist/",
        "build": "yarn build-server && yarn build-client",
        "dev": "concurrently --names \"server,client\" --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "prestart": "yarn build",
        "start": "node dist/app.js",
        "lint": "eslint ."
    },
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "bootstrap": "^4.0.0",
        "cookie-parser": "^1.4.3",
