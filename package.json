        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "firebase": "^5.3.1",
        "firebase-admin": "^6.0.0",
        "font-awesome": "^4.7.0",
        "formik": "^0.11.11",
        "jquery": "^3.3.1",
        "popper.js": "^1.14.1",
        "querystring": "^0.2.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-router-dom": "^4.2.2",
        "react-slick": "^0.22.2",
        "react-toastify": "^4.0.0-rc.4",
        "reactstrap": "^5.0.0",
        "request": "^2.88.0",
        "slick-carousel": "^1.8.1",
        "spotify-web-sdk": "^0.2.1",
        "yup": "^0.24.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "concurrently": "^3.6.1",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
